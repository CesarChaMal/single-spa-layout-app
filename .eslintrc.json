{
  "env": { "browser": true, "es6": true },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "plugin:vue/essential"],
